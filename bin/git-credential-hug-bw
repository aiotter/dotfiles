#!/bin/bash

while read line; do
  [[ -z "$line" ]] && break
  eval ${line%=*}=${line#*=}
done

case $1 in
  get )
    url="${protocol}://${host}"
    paste -d '=' <(echo 'username'; echo 'password') \
                 <(bw list items --url "$url" | jq ".[].login | select(\"$url\" == .uris[].uri) | .username, .password" | tr -d '"')
    ;;
esac
