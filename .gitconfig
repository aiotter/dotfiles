[user]
	useConfigOnly = true
[core]
	excludesfile = ~/.gitignore_global
	editor = vim
[rebase]
	autosquash = true
[commit]
	verbose = true
[log]
	date = iso
[alias]
	one = log --oneline
	graph = log --graph --pretty=format:'%C(yellow)%h%Creset -%C(auto)%d%Creset %s (%cr) %C(blue)<%an>%Creset' --abbrev-commit --date=relative
	unstage = reset HEAD
	get = !ghq get
	list = !ghq list
[ghq]
	root = ~/dev/src
	user = aiotter
[url "git@github.com:"]
    pushInsteadOf = https://github.com/
